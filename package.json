{
  "name": "cap-sflight-hierarchy",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-agencies": "cds watch --open sflight.hierarchy.agencies/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-employee": "cds watch --open sflight.hierarchy.employee/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/agencies",
    "app/employee"
  ]
}
