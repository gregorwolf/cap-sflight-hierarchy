{
  "_version": "1.81.0",
  "sap.app": {
    "id": "sflight.hierarchy.agencies",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.1",
      "toolsId": "dbef0d6a-82b3-450a-b8c6-58a8be5a9e6b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/ui-agency-hd-/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Agency-manage": {
          "semanticObject": "Agency",
          "action": "manage",
          "title": "{{Agency-manage.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sflight.hierarchy.agencies.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "AgencyList",
          "target": "AgencyList"
        },
        {
          "pattern": "Agency({key}):?query:",
          "name": "AgencyObjectPage",
          "target": "AgencyObjectPage"
        },
        {
          "pattern": "Agency({key})/_Employees({key2}):?query:",
          "name": "EmployeeObjectPage",
          "target": "EmployeeObjectPage"
        }
      ],
      "targets": {
        "AgencyList": {
          "type": "Component",
          "id": "AgencyList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Agency",
              "variantManagement": "Page",
              "navigation": {
                "Agency": {
                  "detail": {
                    "route": "AgencyObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "AgencyObjectPage": {
          "type": "Component",
          "id": "AgencyObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Agency",
              "navigation": {
                "_Employees": {
                  "detail": {
                    "route": "EmployeeObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "_Employees/@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "TreeTable",
                    "hierarchyQualifier": "EmployeeHierarchy",
                    "selectionMode": "Auto"
                  }
                }
              }
            }
          }
        },
        "EmployeeObjectPage": {
          "type": "Component",
          "id": "EmployeeObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Agency/_Employees"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}